			}
		},
		{
			"scope": "meta.structure.dictionary.key.python",
			"settings": {
				"foreground": "#B8A8E6"
			}
		},
		{
			"scope": "meta.diff.header",
			"settings": {
				"foreground": "#B8A8E6"
			}
		},
		{
			"scope": "storage",
			"settings": {
				"foreground": "#B8A8E6"
			}
		},
		{
			"scope": "storage.type",
			"settings": {
				"foreground": "#B8A8E6"
			}
		},
		{
			"scope": ["storage.modifier", "keyword.operator.noexcept"],
			"settings": {
				"foreground": "#B8A8E6"
			}
		},
		{
			"scope": ["string", "meta.embedded.assembly"],
			"settings": {
				"foreground": "#C97A5F"
			}
		},
		{
			"scope": "string.tag",
			"settings": {
				"foreground": "#C97A5F"
			}
		},
		{
			"scope": "string.value",
			"settings": {
				"foreground": "#C97A5F"
			}
		},
		{
			"scope": "string.regexp",
			"settings": {
				"foreground": "#A65C3D"
			}
		},

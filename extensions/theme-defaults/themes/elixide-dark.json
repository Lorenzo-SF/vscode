	"semanticHighlighting": true,
	"semanticTokenColors": {
		"newOperator": "#B8A8E6",
		"stringLiteral": "#C97A5F",
		"customLiteral": "#E0D0E8",
		"numberLiteral": "#C97A5F"
	}
}



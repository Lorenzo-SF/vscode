			"settings": {
				"foreground": "#B8A8E6"
			}
		},
		{
			"name": "Elixir Keywords",
			"scope": [
				"keyword.control.elixir",
				"keyword.control.module.elixir",
				"keyword.control.protocol.elixir",
				"keyword.control.erlang"
			],
			"settings": {
				"foreground": "#B8A8E6",
				"fontStyle": "bold"
			}
		},
		{
			"name": "Elixir Functions",
			"scope": ["entity.name.function.elixir"],
			"settings": {
				"foreground": "#B8A8E6"
			}
		},
		{
			"name": "Elixir Types and Modules",
			"scope": [
				"entity.name.type.class.elixir",
				"entity.other.inherited-class.elixir"
			],
			"settings": {
				"foreground": "#8B6FA8"
			}
		},
		{
			"name": "Elixir Symbols",
			"scope": [
				"constant.other.symbol.elixir",
				"constant.other.symbol.unquoted.erlang"
			],
			"settings": {
				"foreground": "#C97A5F"
			}
		},
		{
			"name": "Elixir Operators",
			"scope": ["keyword.operator.other.elixir"],
			"settings": {
				"foreground": "#E0D0E8"
			}
		},
		{
			"name": "Elixir Punctuation",
			"scope": [
				"punctuation.separator.method.elixir",
				"punctuation.section.function.elixir"
			],
			"settings": {
